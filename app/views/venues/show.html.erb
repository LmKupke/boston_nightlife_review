<h2><%= @venue.vname %></h2>

<%= render 'review_form' %>
<br>
<br>

<% if @reviews != nil || @reviews.empty? == false %>
  <% @reviews.each do |review| %>
    <div class="review">
        <%= review.user.username %>
        <% d = review.created_at %>
        <%= d.strftime("%D") %><br>

        Rating: <%= review.venue_rating.to_i %>
        <p><%= review.description %></p>
    </div>
  <% end %>
<% end  %>
